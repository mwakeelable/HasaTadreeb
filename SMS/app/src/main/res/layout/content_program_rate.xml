                    android:layout_height="wrap_content"
                    android:text="3-4 توفير التجهيزات المكتبية"
                    android:layout_marginTop="6dp"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentStart="true"
                    android:textColor="@color/colorPrimaryDark"
                    android:textSize="15sp"/>

                <RadioGroup
                    android:id="@+id/radioNum14"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:layout_alignParentEnd="true"
                    android:layout_alignParentRight="true"
                    android:layout_below="@id/txtNum14Label">

                    <RadioButton
                        android:id="@+id/radioExcNum14"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="ممتاز"
                        android:textSize="12sp" />

                    <RadioButton
                        android:id="@+id/radioVeryGoodNum14"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="جيد جدا"
                        android:textSize="12sp" />

                    <RadioButton
                        android:id="@+id/radioGoodNum14"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="جيد"
                        android:textSize="12sp" />

                    <RadioButton
                        android:id="@+id/radioPassNum14"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="مقبول"
                        android:textSize="12sp" />

                </RadioGroup>
            </RelativeLayout>

            <android.support.v7.widget.CardView
                android:id="@+id/btnSubmitRate"
                android:layout_width="match_parent"
                android:layout_height="55dp"
                android:layout_marginBottom="10dp"
                android:layout_marginTop="10dp"
                app:cardCornerRadius="4dp"
                app:cardUseCompatPadding="true">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@color/grey_300"
                    android:gravity="center"
                    android:orientation="vertical">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="اعتماد التقييم"
                        android:textColor="@color/dark_green"
                        android:textSize="25sp"
                        android:textStyle="bold" />
                </LinearLayout>
            </android.support.v7.widget.CardView>

        </LinearLayout>

    </ScrollView>

</LinearLayout>
